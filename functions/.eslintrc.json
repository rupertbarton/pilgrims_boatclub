{
    "extends": [
        "airbnb/base"
    ],
    "parserOptions": {
        "ecmaVersion": 11
    },
    "rules": {
        "import/order": [
            "error",
            {
              "groups": ["builtin", "external", "internal"],
              "newlines-between": "always",
              "alphabetize": {
                "order": "asc",
                "caseInsensitive": true
              }
            }
          ]
    }
}
